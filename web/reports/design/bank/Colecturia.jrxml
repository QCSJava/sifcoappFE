	<queryString>
		<![CDATA[select t0.linenum, t0.dscription, coalesce(q0.monto,0.0) monto
from ges_col2_colecturiaconcept t0 left join
(
	select t1.linenum, sum(t1.paidsum) as monto
	from  ges_col1_colecturiadetail t1
	join ges_col0_colecturia t2  on t2.docentry = t1.docentry
	join adm_usr0_user t3 on t2.usersign = t3.usersign
	where t2.docdate >= $P{pdocdate} and t2.docdate <= $P{PDOCDATE2} and  t3.nickname = $P{user}
	and t2.transtype = 1 and t2.series = 1
	group by t1.linenum) q0
on t0.linenum = q0.linenum
order by t0.linenum]]>
	</queryString>
